{
  "name": "delivc-sdk-core",
  "version": "0.0.0",
  "description": "Core modules for the Delivc JS SDK",
  "homepage": "https://docs.developer.delivc.com/javascript",
  "main": "dist/index.js",
  "module": "dist/index.es-modules.js",
  "browser": {
    "os": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/delivc/delivc-sdk-core.git"
  },
  "author": "Julian Koehn <me@julian.pro>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf coverage && rimraf dist",
    "build": "yarn clean && rollup -c",
    "lint": "eslint --ignore-path .gitignore ./src/**/*",
    "test": "jest"
  },
  "engines": {
    "node": ">=6"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.23",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.22.0",
    "ts-jest": "^26.1.3",
    "tslib": "^2.0.0",
    "typescript": "^3.9.7"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
